    "import os\n",
    "from tqdm import tqdm\n",
    "\n",
    "# ----------------------------------------------------------\n",
    "# CONFIGURA√á√ÉO DO CLOUDINARY\n",
    "# ----------------------------------------------------------\n",
    "cloudinary.config(\n",
    "    cloud_name=\"dr0dzla6q\",\n",
    "    api_key=\"752615736771352\",\n",
    "    api_secret=\"Xgeumi4tusHT_-SHGyaFJ6qcR_U\",\n",
    "    secure=True\n",
    ")\n",
    "\n",
    "# ----------------------------------------------------------\n",
    "# FUN√á√ÉO PARA BAIXAR UMA PASTA COMPLETA\n",
    "# ----------------------------------------------------------\n",
    "def baixar_pasta_cloudinary(folder_name, pasta_saida=\"./baixados_cloudinary\"):\n",
    "    os.makedirs(pasta_saida, exist_ok=True)\n",
    "\n",
    "    print(f\"\\nüîç Listando arquivos da pasta: {folder_name}\")\n",
    "\n",
    "    recursos = []\n",
    "    next_cursor = None\n",
    "\n",
    "    # Pagina√ß√£o autom√°tica da API\n",
    "    while True:\n",
    "        result = cloudinary.api.resources(\n",
    "            type=\"upload\",\n",
    "            prefix=folder_name + \"/\",   # garante que s√≥ pega a pasta\n",
    "            max_results=500,\n",
    "            next_cursor=next_cursor\n",
    "        )\n",
    "\n",
    "        recursos.extend(result.get(\"resources\", []))\n",
    "        next_cursor = result.get(\"next_cursor\")\n",
    "\n",
    "        if not next_cursor:\n",
    "            break\n",
    "\n",
    "    print(f\"üì¶ Total de arquivos encontrados: {len(recursos)}\\n\")\n",
    "\n",
    "    # -----------------------------------------------------\n",
    "    # DOWNLOAD DOS ARQUIVOS\n",
    "    # -----------------------------------------------------\n",
    "    for recurso in tqdm(recursos, desc=\"Baixando arquivos\", unit=\"arquivo\"):\n",
    "        url = recurso[\"secure_url\"]\n",
    "        nome_arquivo = recurso[\"public_id\"].split(\"/\")[-1] + \".\" + recurso[\"format\"]\n",
    "        caminho = os.path.join(pasta_saida, nome_arquivo)\n",
    "\n",
    "        # baixar\n",
    "        try:\n",
    "            resp = requests.get(url, stream=True)\n",
    "            resp.raise_for_status()\n",
    "\n",
    "            with open(caminho, \"wb\") as f:\n",
    "                for chunk in resp.iter_content(chunk_size=8192):\n",
    "                    if chunk:\n",
    "                        f.write(chunk)\n",
    "\n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Erro ao baixar {url}: {e}\")\n",
    "\n",
    "    print(f\"\\n‚úÖ Pasta baixada com sucesso em: {pasta_saida}\")\n",
    "\n",
    "\n",
    "# ----------------------------------------------------------\n",
    "# EXEMPLO DE USO\n",
    "# ----------------------------------------------------------\n",
    "\n",
    "baixar_pasta_cloudinary(\n",
    "    folder_name=\"imagens_otimizadas_marketplace\",   # nome da pasta no Cloudinary\n",
    "    pasta_saida=\"/home/lucas-silva/auto_shopee/imagens_cloudinary\"  # onde salvar\n",
    ")\n"
   ]
