    "import pandas as pd\n",
    "import math\n",
    "import os\n",
    "\n",
    "def fatiar_excel(arquivo, linhas_por_parte=999, pasta_saida=\"partes\"):\n",
    "    # Carregar o arquivo\n",
    "    df = pd.read_excel(arquivo)\n",
    "\n",
    "    # Criar pasta para salvar\n",
    "    os.makedirs(pasta_saida, exist_ok=True)\n",
    "\n",
    "    # Quantidade de partes\n",
    "    total_linhas = len(df)\n",
    "    total_partes = math.ceil(total_linhas / linhas_por_parte)\n",
    "\n",
    "    # Cabe√ßalho\n",
    "    colunas = df.columns\n",
    "\n",
    "    print(f\"Total de linhas: {total_linhas}\")\n",
    "    print(f\"Gerando {total_partes} arquivos...\")\n",
    "\n",
    "    # Loop para criar as partes\n",
    "    for i in range(total_partes):\n",
    "        inicio = i * linhas_por_parte\n",
    "        fim = inicio + linhas_por_parte\n",
    "\n",
    "        parte_df = df.iloc[inicio:fim]\n",
    "\n",
    "        nome_arquivo = os.path.join(pasta_saida, f\"parte_{i+1}.xlsx\")\n",
    "\n",
    "        # Salvar mantendo o cabe√ßalho\n",
    "        parte_df.to_excel(nome_arquivo, index=False)\n",
    "\n",
    "        print(f\"Arquivo salvo: {nome_arquivo}\")\n",
    "\n",
    "    print(\"Finalizado!\")\n",
    "\n",
    "# Exemplo de uso:\n",
    "fatiar_excel(\"/home/lucas-silva/auto_shopee/bling.xlsx\", linhas_por_parte=999)\n"
   ]
