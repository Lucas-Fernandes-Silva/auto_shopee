    "import os\n",
    "import re\n",
    "import cloudinary\n",
    "import cloudinary.uploader\n",
    "import pandas as pd\n",
    "from PIL import Image\n",
    "from tqdm import tqdm\n",
    "from concurrent.futures import ThreadPoolExecutor\n",
    "import json\n",
    "\n",
    "# -------------- CONFIGURA√á√ïES -------------- #\n",
    "PASTA_LOCAL = \"/home/lucas-silva/auto_shopee/src/extract/img_extract/shopee_imagens\"\n",
    "PASTA_CLOUDINARY = \"produtos\"                        # Nome da pasta no Cloudinary\n",
    "CSV_SAIDA = \"/home/lucas-silva/auto_shopee/urls_cloudinary_shopee.csv\"  # Arquivo final CSV\n",
    "THREADS = 8                                          # Acelera√ß√£o do upload\n",
    "\n",
    "EXTENSOES = (\".jpg\", \".jpeg\", \".png\", \".webp\")\n",
    "# ------------------------------------------- #\n",
    "\n",
    "\n",
    "with open(\"/home/lucas-silva/auto_shopee/src/extract/img_extract/json_files/cloudinary_config.json\", \"r\") as f:\n",
    "    cfg = json.load(f)\n",
    "\n",
    "cloudinary.config(\n",
    "    cloud_name=cfg[\"cloud_name\"],\n",
    "    api_key=cfg[\"api_key\"],\n",
    "    api_secret=cfg[\"api_secret\"],\n",
    "    secure=True,\n",
    ")\n",
    "\n",
    "\n",
    "# -------------- FUN√á√ïES ---------------- #\n",
    "\n",
    "def limpar_nome(nome):\n",
    "    \"\"\"Remove caracteres inv√°lidos e limita a 60 caracteres.\"\"\"\n",
    "    nome = os.path.splitext(nome)[0]  # Remove extens√£o\n",
    "    nome = nome.lower()\n",
    "    nome = re.sub(r'[^a-z0-9_-]+', \"_\", nome)\n",
    "    return nome[:60]\n",
    "\n",
    "\n",
    "def converter_para_jpg(caminho):\n",
    "    \"\"\"Converte a imagem para JPG antes do upload.\"\"\"\n",
    "    novo_caminho = caminho.rsplit(\".\", 1)[0] + \".jpg\"\n",
    "\n",
    "    try:\n",
    "        img = Image.open(caminho).convert(\"RGB\")\n",
    "        img.save(novo_caminho, \"JPEG\", quality=90)\n",
    "        return novo_caminho\n",
    "    except Exception as e:\n",
    "        print(f\"Erro ao converter {caminho}: {e}\")\n",
    "        return None\n",
    "\n",
    "\n",
    "def processar_upload(arquivo):\n",
    "    \"\"\"Processa nome ‚Üí converte ‚Üí envia ‚Üí retorna dados para o CSV.\"\"\"\n",
    "    caminho = os.path.join(PASTA_LOCAL, arquivo)\n",
    "\n",
    "    # Converter para JPG\n",
    "    if not arquivo.lower().endswith(\".jpg\"):\n",
    "        novo = converter_para_jpg(caminho)\n",
    "        if novo is None:\n",
    "            return None\n",
    "        caminho = novo\n",
    "        arquivo_final = os.path.basename(novo)\n",
    "    else:\n",
    "        arquivo_final = arquivo\n",
    "\n",
    "    # Nome limpo\n",
    "    nome_limpo = limpar_nome(arquivo_final)\n",
    "\n",
    "    try:\n",
    "        upload = cloudinary.uploader.upload(\n",
    "            caminho,\n",
    "            folder=PASTA_CLOUDINARY,\n",
    "            public_id=nome_limpo,\n",
    "            overwrite=True\n",
    "        )\n",
    "\n",
    "        return {\n",
    "            \"arquivo_final\": nome_limpo + \".jpg\",\n",
    "            \"url\": upload[\"secure_url\"]\n",
    "        }\n",
    "    except Exception as e:\n",
    "        print(f\"‚ùå Erro ao enviar {arquivo}: {e}\")\n",
    "        return None\n",
    "\n",
    "\n",
    "# -------------- PROCESSAMENTO ---------------- #\n",
    "\n",
    "arquivos = [f for f in os.listdir(PASTA_LOCAL) if f.lower().endswith(EXTENSOES)]\n",
    "resultados = []\n",
    "\n",
    "print(f\"\\nüì§ Enviando {len(arquivos)} imagens para Cloudinary...\\n\")\n",
    "\n",
    "with ThreadPoolExecutor(max_workers=THREADS) as executor:\n",
